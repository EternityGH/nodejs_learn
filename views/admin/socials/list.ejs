<!DOCTYPE html>
<html lang="en">
<%-include ('../includes/head.ejs')%>
<body>
    <main>
        <div class="nps-container-fluid">
            <div class="nps-row m-0">
                <div class="nps-col-3">
                    <aside>
                        <%-include ('../includes/sidebar.ejs')%>
                    </aside>
                </div>
                <div class="nps-col-9">
                    <section class="content">
                        <%-include ('../includes/title-admin.ejs')%>
                        <table>
                            <tr>
                                <th>ID</th>
                                <th>Thứ tự</th>
                                <th>Trang đích</th>
                                <th>url hình</th>
                                <th>có hiển thị?</th>
                                <th>Sửa/xóa</th>
                            </tr>
                            <%data.forEach(_data=>{%>
                            
                                <tr>
                                    <td><%=_data._id%></td>
                                    <td><%=_data.ordering%></td>
                                    <td><%=_data.target_url%></td>
                                    <td><%=_data.img_url%></td>
                                    <td>
                                        <%if(_data.display){%>
                                            Có
                                        <%}else{%>
                                            Không    
                                        <%}%>

                                    </td>
                                    <td>
                                        <a href="/admin/<%=adminPage%>/edit/<%=_data._id%>"><i class="fas fa-pencil-alt"></i></a>
                                        <form method="POST" action="/admin/socials/delete">
                                            <input type="text" name="id" value="<%=_data._id%>" style="display: none;">
                                            <button type="submit"><i class="fas fa-trash-alt delete"></i></button>
                                        </form>
                                    </td>
                                </tr>
                                
                            <%})%>
                        </table>
                    </section>
                </div>
            </div>
        </div>
    </main>
</body>
<%-include ('../includes/js.ejs')%>
<script>
</script>
</html>